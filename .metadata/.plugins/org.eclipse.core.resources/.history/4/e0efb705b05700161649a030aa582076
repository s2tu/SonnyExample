package com.CollectionsExample;

public class Application {

  public static void main(String[] args) {
   DonorImpl donorManager1 = new DonorImpl();
   Donor sonny = new Donor(0, "Sonny", 905540124, "tusonny@asd;laskd.com");
   Donor sonny2 = new Donor(1, "Sonny2", 905540124, "tusonny@asd;laskd.com2");
   Donor sonny3 = new Donor(2, "Sonny3", 905540124, "tusonny@asd;laskd.com3");
   Donor sonny4 = new Donor(3, "Sonny4", 905540124, "tusonny@asd;laskd.com4");
   
   donorManager1.addDonor(sonny);
   donorManager1.printDonorList();
  
   donorManager1.addDonors(sonny2, sonny3, sonny4);
   donorManager1.printDonorList();

  }

}
