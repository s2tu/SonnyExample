package com.training.domains;

public class PrintGrades { //this is a defect because of the 35 case is not checked
	public String findResult(int mark){
		String grade="O";
		if(mark<=35){
			grade="D";
		}else if(mark > 35){
			grade="C";
		}else if(mark < 60){
			grade="C";
		}else{	
			grade="B";
		}
		return grade;
	}
}
