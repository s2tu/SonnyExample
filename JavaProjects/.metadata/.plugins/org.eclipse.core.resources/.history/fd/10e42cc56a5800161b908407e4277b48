package donorInfo;

import java.util.ArrayList;

/**
 * 
 * @author stu1
 * Keeps the data associated to a donor.
 */
public class Donor {
	private int donorID;
	private String name;
	private String email;
	private ArrayList<Donation> myDonations;
	/**
	 * @return the donorID
	 */
	public int getDonorID() {
		return donorID;
	}
	/**
	 * @param donorID the donorID to set
	 */
	public void setDonorID(int donorID) {
		this.donorID = donorID;
	}
	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}
	/**
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}
	/**
	 * @return the email
	 */
	public String getEmail() {
		return email;
	}
	/**
	 * @param email the email to set
	 */
	public void setEmail(String email) {
		this.email = email;
	}
	/**
	 * @return the myDonations
	 */
	public ArrayList<Donation> getMyDonations() {
		return myDonations;
	}
	/**
	 * @param myDonations the myDonations to set
	 */
	public void setMyDonations(ArrayList<Donation> myDonations) {
		this.myDonations = myDonations;
	}
}
